{
    "compilerOptions": {
        "target": "es5",
        "module": "esnext",
        "sourceMap": true,
        "inlineSources": true,
        "declaration": true,
        "strict": true,
        "exactOptionalPropertyTypes": true,
        "allowSyntheticDefaultImports": true,
        "verbatimModuleSyntax": true,
        "esModuleInterop": true,
        "importHelpers": true,
        "forceConsistentCasingInFileNames": true,
        "skipLibCheck": true,
        "noImplicitOverride": true,
        "downlevelIteration": true
    }
}
